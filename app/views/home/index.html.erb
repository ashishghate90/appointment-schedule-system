<h1>Doctors List</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
  	<% doctors_list.each_with_index do |doctor, i| %>
	    <tr>
	      <th scope="row"><%= i+1 %></th>
	      <td><%= "#{doctor.first_name} #{doctor.last_name}" %></td>
	    </tr>
	   <% end %>
  </tbody>
</table>